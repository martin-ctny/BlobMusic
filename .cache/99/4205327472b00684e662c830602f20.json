original":{"line":39,"column":3}}],"sources":{"src/model.js":"import App from \"./app\";\nimport * as THREE from \"three\";\n\nexport default class Model {\n  constructor() {\n    this.app = new App();\n    this.scene = this.app.scene.instance;\n    this.position = new THREE.Vector3(0, 0, 0);\n\n    this.setGeometry();\n    this.setMaterial();\n    this.setMesh();\n    this.setPosition();\n  }\n\n  setGeometry() {\n    this.geometry = new THREE.TorusGeometry(1.5, 0.5, 30, 30);\n  }\n\n  setMaterial() {\n    this.material = new THREE.MeshBasicMaterial({\n      color: { r: 0, g: 255, b: 50 },\n    });\n  }\n\n  setMesh() {\n    this.mesh = new THREE.Mesh(this.geometry, this.material);\n    this.scene.add(this.mesh);\n  }\n\n  setPosition() {\n    this.mesh.position.x = this.position.x;\n    this.mesh.position.y = this.position.y;\n    this.mesh.position.z = this.position.z;\n  }\n\n  update() {\n    this.mesh.rotation.x -= 0.01 * this.app.loadSound.kick;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"599e14dda29edac1d2f75de052b6ca16","cacheData":{"env":{}}}